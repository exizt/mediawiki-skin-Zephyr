<header id="header">
	<div class="zpr-container">
		<div class="mw-header">
			{{>Logo}}
			{{>Notifications}}
			{{>Dropdown}}{{>PersonalMenu}}
			{{>Search}}
		</div>
	</div>
</header>
{{>Notices}}
<div class="zpr-content-wrapper zpr-container">
	<main id="content" class="mw-body">
		<header class="content__header">
			{{>content/ContentIndicators}}
			<div class="zpr-content-header">
				{{>content/ContentHeading}}
				{{^is-anon}}
				<div class="zpr-header-actions">
					{{>content/ContentActions2}}
				</div>
				{{/is-anon}}
			</div>
		</header>
		<div id="bodyContent" data-mw-ve-target-container>
		{{>content/ContentBody}}
		</div>
	</main>
	{{>sidebar/Sidebar}}
	{{#data-toc}}{{>TableOfContents}}{{/data-toc}}
</div>
<hr class="zpr-bottomline">
<nav>
	{{>Languages}}
</nav>
{{>Footer}}
